<%= render 'shared/navbar_basic' %>
<div class="container">
  <%= simple_form_for @venue do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :address %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <%= f.input :category, collection: Venue::CATEGORIES %>
    <%= f.button :submit, 'SAVE', class: 'btn-block btn-lp' %>
  <% end %>

</div>
